#include <iostream>
#include <vector>

using namespace std;

int main() {
    
    
    int x;
    int K;
    cin >> K;
    
    vector<int> v;
    v.push_back(K)
    
    for(int i = 0; i < K; i++){
        cin >> x;
        v.push_back(x);
    }
    
    
    cout << v[3] << endl;
    
    
	return 0;
}
