#include <iostream>
#include <vector>

using namespace std;

int main() {
    
    
    int x;
    int K;
    cin >> K;
    
    vector<int> v;
    v.push_back(K)
    
    for(int i = 0; i < K; i++){
        cin >> x;
        v.push_back(x);
    }
    
    
    cout << v[3] << endl;
    
    
	return 0;
}




#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    
    int N;
    int x;
    int K;
    cin >> N;   
    
    cin >> K;
    vector<int> v;
    for(int i = 0; i < K; i++){
        cin >> x;
        v.push_back(x);
    }
    for(int i = 0; i < K; i++){
        cout << v[i];
    }
    cout << endl;
    sort(v.begin(), v.end());
    for(int i = 0; i < K; i++){
        cout << v[i];
    }
    cout << endl;    
    
	return 0;
}
