#include <iostream>
using namespace std;

int main() {
    int n;
    int t = 0;
    cin >> n;
    for(int i = 2; i < n; i=i+1){
        if(n%i == 0){
            t=t+1;
        }
    }
    if(t==0){
       cout << n << " e primo" << endl; 
    }
    if(t!=0){
        cout << n << " nao e primo" << endl;
    }
	return 0;
}
